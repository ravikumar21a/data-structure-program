#include<iostream>
using namespace std;
int main(){
 int arr[10],n,x;
 cout<<"enter the size of array: ";
 cin>>n;
 cout<<"enter array element :";
 for(int i=0;i<n;i++){
  cin>>arr[i];
 }
 cout<<"insert tha array bigninng element:";
 cin>>x;
 for(int i=n; i>0; i--){
  arr[i]=arr[i-1];
 }
 arr[0]=x;
 n++;
 cout<<"array element are:";
 for(int i=0; i<n; i++){
  cout<<arr[i]<<endl;
 }
 return 0;
}
